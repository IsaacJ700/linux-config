{
    "cSpell.userWords": [
        "camkes",
        "dorner",
        "gvsu",
        "gvsu's",
        "kredit",
        "mkdir",
        "passthrough",
        "qemu",
        "repo",
        "userspace",
        "zynq"
    ],
    "editor.wordWrapColumn": 100,
    "files.exclude": {
        "**/*.un~": true,
        "**/build": true
    },
    "editor.tabCompletion": "on",
    "editor.suggest.localityBonus": true,
    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\wsl.exe",
    "terminal.integrated.shellArgs.linux": [ "-l" ],
    "terminal.external.linuxExec": "xterm-256color",
    "workbench.colorCustomizations": {
        // The background is optional
        "terminal.background": "#282a36",
        "terminalCursor.background": "#282a36",
        "terminalCursor.foreground": "#97979b",
        "terminal.selectionBackground": "#97979b33",
        "terminal.foreground": "#eff0eb",
        "terminal.ansiBlack": "#282a36",
        "terminal.ansiRed": "#b24b48",
        "terminal.ansiGreen": "#40b165",
        "terminal.ansiYellow": "#f3f99d",
        "terminal.ansiBlue": "#57c7ff",
        "terminal.ansiMagenta": "#ff6ac1",
        "terminal.ansiCyan": "#9aedfe",
        "terminal.ansiWhite": "#f1f1f0",
        "terminal.ansiBrightBlack": "#686868",
        "terminal.ansiBrightRed": "#ff6c67",
        "terminal.ansiBrightGreen": "#73e498",
        "terminal.ansiBrightYellow": "#f3f99d",
        "terminal.ansiBrightBlue": "#57c7ff",
        "terminal.ansiBrightMagenta": "#ff6ac1",
        "terminal.ansiBrightCyan": "#9aedfe",
        "terminal.ansiBrightWhite": "#eff0eb"
    },
    "git.path": "C:\\wsl\\bin\\wslgit.exe",
    "editor.minimap.maxColumn": 100,
    "editor.minimap.renderCharacters": false,
    "window.newWindowDimensions": "inherit",
    "scm.diffDecorationsGutterWidth": 2,
    "editor.rulers": [
        100
    ],
    "[python]": {
        "editor.rulers": [
            79,
            120
        ]
    },
    "simpleVim.normalModeCursorStyle": "block",
    "search.useGlobalIgnoreFiles": true,
    "editor.smoothScrolling": true,
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Solarized Light",
    "workbench.iconTheme": "vscode-icons",
    "explorer.confirmDelete": false,
    "vsicons.dontShowNewVersionMessage": true,
    "latex-workshop.view.pdf.viewer": "tab",
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cmake",
        "cpp",
        "csharp",
        "css",
        "git-commit",
        "go",
        "handlebars",
        "haskell",
        "html",
        "jade",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "pug",
        "python",
        "restructuredtext",
        "rust",
        "scala",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yaml",
        "yml"
    ],
    "editor.wrappingIndent": "deepIndent",
    "markdown.extension.italic.indicator": "_",
    "editor.minimap.enabled": false,
    "files.associations": {
        "*.camkes": "c"
    },
    "editor.fontLigatures": true,
    "editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
    "markdown.extension.toc.githubCompatibility": true,
    "search.location": "panel",
    "simpleVim.visualLineModeCursorStyle": "underline",
    "simpleVim.visualModeCursorStyle": "blockOutline",
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.views.fileHistory.location": "scm",
    "gitlens.views.lineHistory.location": "scm",
    "codewall.openProblemsPane": false,
    "readTime.fileTypes": [
        "markdown",
        "latex"
    ],
    "C_Cpp.default.cStandard": "c99",
    "[latex]": {
        "editor.wordWrap": "on"
    },
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.defaultFormatter": "Glavin001.unibeautify-vscode",
    },
    "terminal.integrated.rendererType": "dom",
    "[json]": {
        "editor.defaultFormatter": "Glavin001.unibeautify-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "Glavin001.unibeautify-vscode"
    },
    "settings.cycle": [
        {
            "id": "colorTheme",
            "overrideWorkspaceSettings": true,
            "values": [
                {
                    "workbench.colorTheme": "Solarized Light"
                },
                {
                    "workbench.colorTheme": "Monokai Dimmed"
                }
            ]
        },
        {
            "id": "minimap",
            "overrideWorkspaceSettings": true,
            "values": [
                {
                    "editor.minimap.enabled": false
                },
                {
                    "editor.minimap.enabled": true
                }
            ]
        },
        {
            "id": "ligatures",
            "overrideWorkspaceSettings": true,
            "values": [
                {
                    "editor.fontLigatures": true
                },
                {
                    "editor.fontLigatures": false
                }
            ]
        },
    ],
}
